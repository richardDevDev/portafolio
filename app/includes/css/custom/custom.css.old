@-webkit-keyframes flipHeads {
  from { -webkit-transform: rotateY(0); -moz-transform: rotateY(0); transform: rotateY(0); }
  to { -webkit-transform: rotateY(1800deg); -moz-transform: rotateY(1800deg); transform: rotateY(1800deg); }
}
@-webkit-keyframes flipTails {
  from { -webkit-transform: rotateY(0); -moz-transform: rotateY(0); transform: rotateY(0); }
  to { -webkit-transform: rotateY(1980deg); -moz-transform: rotateY(1980deg); transform: rotateY(1980deg); }
}


@keyframes anim{

    0% {background-color: #61CAE4;} /*Amarillo*/
   15% {background-color: #64B45B;} /*Naranja*/
   30% {background-color: #F6CB53;} /*Negro*/
   45% {background-color: #B72831;}
   60% {background-color: #9D5CA1;} /*Otra vez naranja*/
   75% {background-color: #E65C31;}
  100% {background-color: #61CAE4;} /*Otra vez amarillo*/
}
.anim {
  animation-name: anim;
  animation-duration: 15s;
  animation-iteration-count: infinite;
}


